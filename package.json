{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "seed-classifieds": "node scripts/seedClassifieds.js",
    "seed-users": "node scripts/seedUsers.js",
    "seed-events": "node scripts/seedEvents.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "css-loader": "^3.2.0",
    "moment": "^2.24.0",
    "nodemon": "^1.18.7",
    "react-big-calendar": "^0.22.0",
    "style-loader": "^1.0.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "calendar-base": "^0.3.0",
    "connect-mongo": "^3.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-session": "^1.16.2",
    "if-env": "^1.0.4",
    "leaflet": "^1.5.1",
    "mongoose": "^5.6.8",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-event-calendar": "^0.3.0",
    "react-leaflet": "^2.4.0",
    "react-moment": "^0.9.2",
    "redux": "^4.0.4",
    "video-react": "^0.14.1"
  }
}
